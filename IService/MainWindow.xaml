<Window x:Class="IService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IService"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label Content="Controller Name"></Label>
            <TextBox x:Name="controller"></TextBox>
            <Label Content="Action Name"></Label>
            <TextBox x:Name="action"></TextBox>

            <Button Content="Save" x:Name="save"></Button>
            
            <Label Content="Account"></Label>
            <TextBox x:Name="account"></TextBox>
            <Label Content="Password"></Label>
            <TextBox x:Name="password"></TextBox>

            <Button Content="Register" x:Name="regis"></Button>
            <Button Content="Listen" x:Name="listen"></Button>
        </StackPanel>
        
    </Grid>
</Window>
